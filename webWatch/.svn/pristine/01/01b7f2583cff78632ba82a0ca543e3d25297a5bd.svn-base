                                    <div class="product-bottom">
                                        <h3>${w.title}</h3>
                                        <p>${w.style.des}</p>
                                        <h4><a class="item_add" href="#"><i></i></a> <span class=" item_price">$ ${w.price}</span></h4>
                                    </div>
                                    <div class="srch srch1">
                                        <span>-50%</span>
                                    </div>
                                </div>
                            </div>
                        </c:forEach>
                                <div class="dataTables_paginate paging_simple_numbers"
                                     id="editable_paginate">
                                    <div align="center">
                                        当前第 <label> ${requestScope.pageNo} </label> 页
                                    </div>
                                    <ul class="pagination">
                                        <li class="paginate_button previous editable"
                                            aria-controls="editable" tabindex="0"
                                            id="editable_previous"><a
                                                href="/watch/products/${requestScope.styleDes}/${requestScope.type}?pageNo=${requestScope.pageNo-1}">上一页</a>
                                        </li>

                                        <li class="paginate_button next" aria-controls="editable"
                                            tabindex="0" id="editable_next"><a
                                                href="/watch/products/${requestScope.styleDes}/${requestScope.type}?pageNo=${requestScope.pageNo+1}">下一页</a>
                                        </li>
                                    </ul>
                                    <div class="row">
                                        <form class = "form-group" action = "/watch/products/${requestScope.styleDes}/${requestScope.type}" method="get">
                                            <div class = "col-xs-6">
                                                <input type="text" name="pageNo" class="form-control">
                                            </div>

                                            <div class = "col-xs-6">
                                                <input type="submit"value="GO" class="form-control"/>
                                            </div>
                                        </form>

                                    </div>
                                </div>

                    </div>
                </c:when>
                <c:otherwise>
                    <h2>该品牌暂时缺货,请联系商家</h2>
                </c:otherwise>
            </c:choose>
            <div class="col-md-3 prdt-right">
